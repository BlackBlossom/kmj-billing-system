{
  "name": "kmj-billing-server",
  "version": "1.0.0",
  "description": "KMJ Billing System - MERN Stack Backend",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node src/scripts/migrate-from-sql.js",
    "migrate:clean": "node src/scripts/clean-migrate.js",
    "migrate:simple": "node src/scripts/simple-migrate.js",
    "migrate:bills": "node src/scripts/migrate-bills-accounts.js",
    "init:counters": "node src/scripts/init-counters.js",
    "check:admin": "node src/scripts/check-admin.js",
    "update:admin-password": "node src/scripts/update-admin-password.js",
    "seed": "node src/scripts/seed.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.js",
    "format": "prettier --write \"src/**/*.js\""
  },
  "keywords": [
    "billing",
    "membership",
    "kmj",
    "mern"
  ],
  "author": "KMJ Admin",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "mongoose": "^8.9.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "dotenv": "^16.4.7",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "multer": "^2.0.2",
    "cloudinary": "^1.41.3",
    "pdfkit": "^0.15.1",
    "nodemailer": "^7.0.10",
    "winston": "^3.17.0",
    "morgan": "^1.10.0",
    "compression": "^1.7.5",
    "cookie-parser": "^1.4.7",
    "express-mongo-sanitize": "^2.2.0",
    "express-xss-sanitizer": "^2.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "eslint": "^9.18.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  }
}
